{
    "family": "backend-task",
    "containerDefinitions": [
      {
        "name": "backend-container",
        "image": "YOUR_AWS_ACCOUNT_ID.dkr.ecr.YOUR_REGION.amazonaws.com/backend:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000
          }
        ]
      }
    ]
  }
  